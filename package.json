{
  "name": "training",
  "version": "0.1.0",
  "description": "Proyecto training",
  "engines": {
    "node": "8.12.0",
    "npm": "2.14.2"
  },
  "scripts": {
    "console": "node console.js",
    "cover": "NODE_ENV=testing istanbul cover ./node_modules/mocha/bin/_mocha --compilers js:babel-core/register test/app.js",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "nsp": "./node_modules/.bin/nsp check",
    "lint": "./node_modules/eslint/bin/eslint.js \"**/*.js\"",
    "lint-diff": "git diff --name-only --cached --relative | grep \\\\.js$ | xargs ./node_modules/eslint/bin/eslint.js",
    "lint-fix": "./node_modules/eslint/bin/eslint.js \"**/*.js\" --fix",
    "precommit": "npm run lint-diff",
    "outdated": "npm outdated --depth 0",
    "pretest": "npm run lint",
    "prestart": "npm run lint",
    "migrations": "./node_modules/.bin/sequelize db:migrate",
    "start": "./node_modules/.bin/nodemon --inspect app.js",
    "test": "NODE_ENV=testing ./node_modules/mocha/bin/_mocha --timeout 6000 --exit --compilers js:babel-core/register test/app.js && npm run nsp",
    "test-inspect": "NODE_ENV=testing node --inspect --debug-brk ./node_modules/mocha/bin/_mocha --compilers js:babel-core/register test/app.js"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "main": "app.js",
  "author": "Wolox",
  "homepage": "https://github.com/Wolox/training",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Wolox/training.git"
  },
  "bugs": {
    "url": "https://github.com/Wolox/training/issues"
  },
  "dependencies": {
    "Faker": "^0.7.2",
    "ajv": "^6.5.4",
    "bcrypt": "^3.0.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "eslint-config-loopback": "^13.1.0",
    "expect": "^24.7.1",
    "express": "^4.16.4",
    "express-graphql": "^0.8.0",
    "factory-girl": "^5.0.2",
    "factory-girl-sequelize": "^1.0.1",
    "faker": "^4.1.0",
    "graphql": "^14.2.1",
    "hashcode": "^1.0.3",
    "jest": "^24.7.1",
    "jsonwebtoken": "^8.3.0",
    "jwt-simple": "^0.5.1",
    "lint": "^1.1.2",
    "moment": "^2.22.2",
    "morgan": "^1.9.1",
    "nock": "^10.0.2",
    "pg": "^7.4.1",
    "pg-native": "^2.2.0",
    "request-promise": "^4.2.4",
    "rollbar": "^2.3.9",
    "sequelize": "^4.34.0",
    "sinon": "^7.0.0",
    "supertest": "^4.0.2",
    "umzug": "^2.1.0",
    "winston": "^2.4.0",
    "winston-daily-rotate-file": "^1.7.2"
  },
  "devDependencies": {
    "babel": "6.23.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-preset-es2015": "6.24.1",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "coveralls": "^3.0.0",
    "dictum.js": "^1.0.4",
    "dotenv": "^5.0.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^12.0.2",
    "eslint-config-prettier": "^2.3.1",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-prettier": "^2.1.1",
    "husky": "^0.14.3",
    "istanbul": "^0.4.3",
    "lite-server": "^2.4.0",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.2.0",
    "nodemon": "^1.18.4",
    "nsp": "^3.2.1",
    "prettier": "^1.8.2",
    "prettier-eslint": "^8.2.1",
    "prompt": "^1.0.0",
    "sequelize-cli": "^5.4.0",
    "sequelize-test-helpers": "^1.0.6"
  }
}
